<?xml version="1.0" encoding="utf-8" ?>
<widget
    id="scaletech.charts.Charts"
    pluginWidget="true"
    needsEntityContext="true"
    offlineCapable="true"
    supportedPlatform="Web"
    xmlns="http://www.mendix.com/widget/1.0/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../node_modules/mendix/custom_widget.xsd"
>
    <name>Charts</name>
    <description>My widget description</description>
    <icon>
        
    </icon>
    <properties>
        <propertyGroup caption="General">
            <propertyGroup caption="General">
                <property key="objectsDatasource" type="datasource" isList="true">
                    <caption>Data Source</caption>
                    <description>Define the data source for chart values.</description>
                </property>

                <property key="ChartType" type="enumeration" defaultValue="bar">
                    <caption>Defult Chart Type</caption>
                    <description>Specifies the default chart type to render (e.g., bar, line, pie).</description>
                    <enumerationValues>
                        <enumerationValue key="bar">Bar</enumerationValue>
                        <enumerationValue key="line">Line</enumerationValue>
                        <enumerationValue key="pie">Pie</enumerationValue>
                        <enumerationValue key="doughnut">Doughnut</enumerationValue>
                        <enumerationValue key="polarArea">Polar Area</enumerationValue>
                        <enumerationValue key="radar">Radar</enumerationValue>
                        <enumerationValue key="scatter">Scatter</enumerationValue>
                        <enumerationValue key="bubble">Bubble</enumerationValue>
                    </enumerationValues>
                </property>

                <property key="XaxisData" type="attribute" dataSource="objectsDatasource">
                    <caption>X Axis</caption>
                    <description>Defines the attribute in the data source for X-axis values.</description>
                    <attributeTypes>
                        <attributeType name="Integer" />
                        <attributeType name="Decimal" />
                    </attributeTypes>
                </property>
                <property key="YaxisData" type="attribute" dataSource="objectsDatasource">
                    <caption>Y Axis</caption>
                    <description>Defines the attribute in the data source for Y-axis values.</description>
                    <attributeTypes>
                        <attributeType name="Integer" />
                        <attributeType name="Decimal" />
                    </attributeTypes>
                </property>
                <property key="bubbleRadius" type="expression" defaultValue="5" dataSource="objectsDatasource">
                    <caption>Bubble Chart Radius</caption>
                    <description>Expression defining the radius size for bubbles in a Bubble chart.</description>
                    <returnType assignableTo="XaxisData" />
                </property>

                <property key="dataKey" type="attribute" dataSource="objectsDatasource">
                    <caption>Data Key</caption>
                    <description>Define the key name in data objects to use for value</description>
                    <attributeTypes>
                        <attributeType name="Integer" />
                        <attributeType name="Decimal" />
                        <attributeType name="String" />
                    </attributeTypes>
                </property>
                <property key="labelKey" type="attribute" dataSource="objectsDatasource">
                    <caption>Label Key</caption>
                    <description>Defines the attribute for labels in the chart</description>
                    <attributeTypes>
                        <attributeType name="Integer" />
                        <attributeType name="Decimal" />
                        <attributeType name="String" />
                    </attributeTypes>
                </property>
                <property key="myToolTip" type="textTemplate" dataSource="objectsDatasource">
                    <caption>Tooltip Template</caption>
                    <description>
                        Tooltip text displayed when hovering over chart elements. Can include dynamic data from the data
                        source.
                    </description>
                    <translations>
                        <translation lang="en_US" />
                    </translations>
                </property>
            </propertyGroup>
            <propertyGroup caption="Chart Type Selection">
                <property key="IsSelection" type="boolean" defaultValue="false">
                    <caption>Show Chart Selection</caption>
                    <description>Enable or disable the display of the chart selection box.</description>
                </property>
                <property key="SelectionBoxLable" type="string" required="false">
                    <caption>Selection Box Label</caption>
                    <description>The title displayed above the treemap chart.</description>
                </property>
            </propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Events">
            <property key="chartOnClickAction" type="action" required="false" dataSource="objectsDatasource">
                <caption>onclick Action</caption>
                <description>Action triggered when a chart element is clicked.</description>
            </property>
        </propertyGroup>
        <propertyGroup caption="Style">
            <propertyGroup caption="Chart Height And Width">
                <property key="Width" type="integer" defaultValue="900">
                    <caption>Width</caption>
                    <description>Enable or disable the display of the chart selection box.</description>
                </property>
                <property key="Height" type="integer" defaultValue="600">
                    <caption>Height</caption>
                    <description>The title displayed above the treemap chart.</description>
                </property>
            </propertyGroup>

            <propertyGroup caption="Other Style">
                <property key="hoverEffectColor" type="string" required="false">
                    <caption>Hover Effect Color</caption>
                    <description>Color displayed when hovering over elements</description>
                </property>
                <property key="BorderWidth" type="integer" defaultValue="1">
                    <caption>Chart Border Width</caption>
                    <description />
                </property>
                <property key="borderColor" type="string" required="false">
                    <caption>Border Color</caption>
                    <description />
                </property>
            </propertyGroup>

            <propertyGroup caption="Chart Title">
                <property key="IsTitle" type="boolean" defaultValue="false">
                    <caption>Show Chart Title</caption>
                    <description>Enable or disable the display of the chart title.</description>
                </property>
                <property key="chartTitle" type="string" required="false">
                    <caption>Chart Title</caption>
                    <description>The title displayed above the treemap chart.</description>
                </property>
                <property key="fontColor" type="string" required="false">
                    <caption>Font Color</caption>
                    <description>Set the font color for the chart title (e.g., '#000000' for black).</description>
                </property>
                <property key="fontFamily" type="string" required="false">
                    <caption>Font Family</caption>
                    <description>Set the font family for the chart title (e.g., Arial, Times New Roman).</description>
                </property>
                <property key="fontSize" type="decimal" defaultValue='12'>
                    <caption>Font Size</caption>
                    <description>Define the font size for the chart title in pixels.</description>
                </property>
                <property key="fontStyle" type="string" required="false">
                    <caption>Font Style</caption>
                    <description>
                        Choose the font style for the chart title (e.g., normal, italic, oblique, initial, inherit).
                    </description>
                </property>
                <property key="fontWeight" type="decimal" defaultValue='100'>
                    <caption>Font Weight</caption>
                    <description>
                        Set the font weight for the chart title (e.g., 100 for light, 400 for normal, 700 for lighter,
                        900 for bolder).
                    </description>
                </property>
            </propertyGroup>
            <propertyGroup caption="X And Y Axis Labels">
                <property key="XaxisLineColor" type="string" required="false">
                    <caption>X Axis Line Color</caption>
                    <description />
                </property>
                <property key="YaxisLineColor" type="string" required="false">
                    <caption>Y Axis Line Color</caption>
                    <description />
                </property>
                <property key="XaxisBorderWidth" type="decimal" defaultValue='1'>
                    <caption>X Axis Border Width</caption>
                    <description />
                </property>
                <property key="YaxisBorderWidth" type="decimal" defaultValue='1'>
                    <caption>Y Axis Border Width</caption>
                    <description />
                </property>
                <property key="labelsFontColor" type="string" required="false">
                    <caption>Font Color</caption>
                    <description>Set the font color for the labels (e.g., '#000000' for black).</description>
                </property>
                <property key="labelsFontFamily" type="string" required="false">
                    <caption>Font Family</caption>
                    <description>Set the font family for the labels (e.g., Arial, Times New Roman).</description>
                </property>
                <property key="labelsFontSize" type="decimal" defaultValue='12'>
                    <caption>Font Size</caption>
                    <description>Define the font size for the labels in pixels.</description>
                </property>
                <property key="labelsFontStyle" type="string" required="false">
                    <caption>Font Style</caption>
                    <description>
                        Choose the font style for the labels (e.g., normal, italic, oblique, initial, inherit).
                    </description>
                </property>
                <property key="labelsFontWeight" type="decimal" defaultValue='100'>
                    <caption>Font Weight</caption>
                    <description>
                        Set the font weight for the labels (e.g., 100 for light, 400 for normal, 700 for lighter, 900
                        for bolder).
                    </description>
                </property>
            </propertyGroup>

            <propertyGroup caption="Line, Scatter And Bubble Chart Style">
                <property key="pointBorderColor" type="string" required="false">
                    <caption>Point Border Color</caption>
                    <description />
                </property>
                <property key="pointBackgroundColor" type="string" required="false">
                    <caption>Point Background Color</caption>
                    <description />
                </property>
            </propertyGroup>
        </propertyGroup>
    </properties>
</widget>
